{
	"cSpell.words": [
		"cloudinary",
		"clsx",
		"fedcm",
		"tailwindcss",
		"tanstack",
		"TRPC",
		"xresume"
	],
	"files.eol": "\n",
	"editor.formatOnSave": true,
